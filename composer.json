{
    "name": "neutronstars/enum",
    "description": "Added an enumeration system as close as possible to PHP 8.1 for earlier versions.",
    "type": "library",
    "license": [ "Apache-2.0" ],
    "authors": [
        {
            "name": "NeutronStars",
            "email": "pro@neutronstars.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 | ^8.0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "NeutronStars\\Enum\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 | ^7.0 | ^8.0 | ^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "NeutronStars\\Enum\\Tests\\": "tests/"
        }
    }
}
