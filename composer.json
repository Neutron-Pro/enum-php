{
    "name": "neutronstars/enum",
    "description": "Added an enumeration system as close as possible to PHP 8.1 for earlier versions.",
    "type": "library",
    "license": [ "Apache-2.0" ],
    "authors": [
        {
            "name": "NeutronStars",
            "email": "pro@neutronstars.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 | ^8.0",
        "doctrine/dbal": "^2.1",
        "symfony/serializer": "^5.2",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "NeutronStars\\Enum\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "NeutronStars\\Enum\\Test\\": "test/"
        }
    }
}
